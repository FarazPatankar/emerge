<h1 class="container" id="title" data-lat="<%=@bootcamp.lat%>" data-lng="<%=@bootcamp.lng%>"><%=@bootcamp.name%></h1>

<img id="logo" src="<%=@bootcamp.avatar%>">

<div id="selector" class="type-holder">
    <form action="#">
      <ul class="type-list">
        <% @types.each do |selectionName|%>
          <% if selectionName == "store" %>          
            <% selectionName = "all" %>
          <% end %>
          <% selectionNameChange = selectionName.gsub(/_/, ' ')%>
          <li>
            <input name="group1" type="radio" class="store" id="<%= selectionName%>"
              <% if selectionName == "all" %>
              checked="checked"
              <% end %>
            />
            <label for="<%=selectionName%>"><i class="tiny material-icons"><%=@icons[selectionName]%></i><%=selectionNameChange%></label>
          </li>
        <% end %>
      </ul>
    </form>
</div>
      

<div class="container" id="map"></div>